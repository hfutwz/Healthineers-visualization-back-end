<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.GcsScoreMapper">

    <!-- 根据患者ID查询GCS评分 -->
    <select id="selectByPatientId" parameterType="java.lang.Integer" resultType="com.demo.entity.GcsScore">
        SELECT * FROM gcs_score WHERE patient_id = #{patientId}
    </select>

</mapper>
